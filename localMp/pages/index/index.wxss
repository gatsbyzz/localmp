/**index.wxss**/
.swiperBox swiper {
  width: 750rpx;
  height: 345rpx;
}
.swiperBox swiper image {
  width: 100%;
}
.nav_box {
  display: flex;
  padding: 10rpx;
}
.nav_box .nav_cat {
  flex: 1;
  padding: 30rpx;
}
.nav_box .nav_cat .catimg {
  width: 100%;
}
.floor_box .floor_group .floor_title {
  padding-top: 10rpx;
}
.floor_box .floor_group .floor_title image {
  width: 100%;
}
.floor_box .floor_group .floor_content navigator {
  overflow: hidden;
  width: 33.3%;
  float: left;
}
.floor_box .floor_group .floor_content navigator:nth-last-child(-n+4) {
  height: 27.70215517vw;
  border-left: 4rpx solid #fff;
}
.floor_box .floor_group .floor_content navigator:nth-child(2),
.floor_box .floor_group .floor_content navigator:nth-child(3) {
  border-bottom: 4rpx solid #fff;
}
.floor_box .floor_group .floor_content navigator image {
  width: 100%;
  height: 100%;
}
